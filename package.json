    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-phone-number-input": "^3.3.12",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2",
